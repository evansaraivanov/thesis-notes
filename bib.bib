@article{gillis_effects_2019,
	title = {The {Effects} of {Calibration} on the {Bias} of {Shear} {Measurements}},
	volume = {482},
	issn = {0035-8711, 1365-2966},
	url = {http://arxiv.org/abs/1809.09540},
	doi = {10.1093/mnras/sty2637},
	abstract = {Forthcoming large-scale surveys will soon attempt to measure cosmic shear to an unprecedented level of accuracy, requiring a similarly high level of accuracy in the shear measurements of galaxies. Factors such as pixelisation, imperfect point-spread function (PSF) correction, and pixel noise can all directly or indirectly lead to biases in shear measurements, and so it can be necessary for shear measurement methods to be calibrated against internal, external, or simulated data to minimize bias. It is thus important to understand the nature of this calibration. In this paper, we show that a typical calibration procedure will on average leave no residual additive bias, but will leave a residual multiplicative bias. Additionally, the errors on the post-calibration bias parameters will be changed, and on average increased, from the errors on the pre-calibration measurements of these parameters, but that this is generally worth the beneÔ¨Åt in decreasing the expected value of the multiplicative bias. We Ô¨Ånd that in most typical cases, it is worthwhile to apply a Ô¨Årst-order bias correction, while a higher-order bias correction is only worthwhile for methods with intrinsically high multiplicative bias ({\textgreater} 10 per cent) or when the simulation size is very small ({\textless} 106 simulated galaxies).},
	language = {en},
	number = {1},
	urldate = {2023-02-07},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Gillis, Bryan R. and Taylor, A. N.},
	month = jan,
	year = {2019},
	note = {arXiv:1809.09540 [astro-ph]},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {402--415},
	file = {Gillis and Taylor - 2019 - The Effects of Calibration on the Bias of Shear Me.pdf:/Users/evan/Zotero/storage/CSSXVF2Q/Gillis and Taylor - 2019 - The Effects of Calibration on the Bias of Shear Me.pdf:application/pdf},
