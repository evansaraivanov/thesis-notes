@misc{elvin-poole_dark_2022,
	title = {Dark {Energy} {Survey} {Year} 3 results: {Magnification} modeling and impact on cosmological constraints from galaxy clustering and galaxy-galaxy lensing},
	shorttitle = {Dark {Energy} {Survey} {Year} 3 results},
	url = {http://arxiv.org/abs/2209.09782},
	abstract = {We study the eÔ¨Äect of magniÔ¨Åcation in the Dark Energy Survey Year 3 analysis of galaxy clustering and galaxy-galaxy lensing, using two diÔ¨Äerent lens samples: a sample of Luminous red galaxies, redMaGiC, and a sample with a redshift-dependent magnitude limit, MagLim. We account for the eÔ¨Äect of magniÔ¨Åcation on both the Ô¨Çux and size selection of galaxies, accounting for systematic eÔ¨Äects using the Balrog image simulations. We estimate the impact of magniÔ¨Åcation on the galaxy clustering and galaxy-galaxy lensing cosmology analysis, Ô¨Ånding it to be a signiÔ¨Åcant systematic for the MagLim sample. We show cosmological constraints from the galaxy clustering auto-correlation and galaxy-galaxy lensing signal with diÔ¨Äerent magniÔ¨Åcations priors, Ô¨Ånding broad consistency in cosmological parameters in ŒõCDM and wCDM. However, when magniÔ¨Åcation bias amplitude is allowed to be free, we Ô¨Ånd the two-point correlations functions prefer a diÔ¨Äerent amplitude to the Ô¨Åducial input derived from the image simulations. We validate the magniÔ¨Åcation analysis by comparing the cross-clustering between lens bins with the prediction from the baseline analysis, which uses only the auto-correlation of the lens bins, indicating systematics other than magniÔ¨Åcation may be the cause of the discrepancy. We show adding the cross-clustering between lens redshift bins to the Ô¨Åt signiÔ¨Åcantly improves the constraints on lens magniÔ¨Åcation parameters and allows uninformative priors to be used on magniÔ¨Åcation coeÔ¨Écients, without any loss of constraining power or prior volume concerns.},
	language = {en},
